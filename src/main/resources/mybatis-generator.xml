<?xmlversion="1.0"encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- ClassPathEntry: database JDBC driven jar packet address -->
    <context id="DB2Tables" targetRuntime="MyBatis3">

        <jdbcConnection>driverClass</jdbcConnection>
        <jdbcConnectiondriverClass>"oracle.jdbc.driver.OracleDriver"
        connectionURL="jdbc:oracle:thin:@198.17.1.1:1521:ORCL"
        userId="unuser"
        password="password">

        <propertyname="forceBigDecimals"value="false"/>
    </javaTypeResolver>
    <!-- Position automatic code generation of targetProject: -->
    <javaModelGeneratortargetPackage="com.soft.model"targetProject="E:\WebWorkSpace\workspace_js\downAttachdemo\src">
